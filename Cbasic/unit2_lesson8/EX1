#include <stdio.h>
#include <stdlib.h>
void main() {
	int m =29;
	printf("the address of  m : %p\nvalue of m : %d\n",&m,m);

	int* ab=&m;
	printf("the address of the pointer ab : %p\nconyent of the pointer ab : %d\n",ab,*ab);
	m=34;
	printf("the address of the pointer ab : %p\nconyent of the pointer ab : %d\n",ab,*ab);
	*ab=7;
	printf("the address of  m : %p\nvalue of m : %d\n",&m,m);
}